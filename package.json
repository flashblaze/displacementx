{
  "name": "displacementx",
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:types": "tsc",
    "test:units": "vitest",
    "test:lint": "next lint --max-warnings=0 --format=compact",
    "test:format": "prettier . --check",
    "format": "prettier . --write",
    "upgrade": "bunx npm-check-updates -u && bun i",
    "pages:build": "bunx @cloudflare/next-on-pages",
    "preview": "bun pages:build && wrangler pages dev",
    "deploy": "bun pages:build && wrangler pages deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv env.d.ts"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "1.1.2",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-radio-group": "1.2.1",
    "@radix-ui/react-slider": "1.2.1",
    "@radix-ui/react-switch": "1.1.1",
    "clsx": "2.1.1",
    "next": "15.0.3",
    "react": "18.3.1",
    "react-colorful": "5.6.1",
    "react-dom": "18.3.1",
    "usehooks-ts": "3.1.0",
    "zustand": "5.0.1"
  },
  "devDependencies": {
    "@cloudflare/next-on-pages": "^1.13.5",
    "@cloudflare/workers-types": "^4.20241112.0",
    "@next/eslint-plugin-next": "15.0.3",
    "@types/node": "22.9.3",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "autoprefixer": "10.4.20",
    "eslint": "9.15.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-xo": "0.46.0",
    "eslint-config-xo-react": "0.27.0",
    "eslint-config-xo-typescript": "7.0.0",
    "eslint-plugin-next-on-pages": "^1.13.5",
    "eslint-plugin-tailwindcss": "3.17.5",
    "happy-dom": "15.11.6",
    "postcss": "8.4.49",
    "prettier": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.9",
    "serve": "14.2.4",
    "tailwindcss": "3.4.15",
    "typescript": "5.7.2",
    "vercel": "^39.1.1",
    "vitest": "2.1.5",
    "wrangler": "^3.90.0"
  }
}
